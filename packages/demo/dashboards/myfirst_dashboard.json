{
  "layout": {
      "title": false,
      "customJS" : ["jquery.peity.js"],
      "widgets" : [
          {"row" : 1, "col" : 1, "width" : 1, "height" : 1, "widget" : "numberDisplay",    "job" : "getCell",         "config": "owedTom"},
          {"row" : 1, "col" : 2, "width" : 1, "height" : 1, "widget" : "numberDisplay",    "job" : "getCell",         "config": "owedRob"},
          {"row" : 1, "col" : 3, "width" : 1, "height" : 1, "widget" : "numberDisplay",    "job" : "getCell",         "config": "owedYutao"},
          {"row" : 1, "col" : 4, "width" : 1, "height" : 1, "widget" : "numberDisplay",    "job" : "getCell",         "config": "owedJohn"},
          {"row" : 1, "col" : 5, "width" : 1, "height" : 1, "widget" : "numberDisplay",    "job" : "getCell",         "config": "owedJacob"},
          {"row" : 1, "col" : 6, "width" : 1, "height" : 1, "widget" : "numberDisplay",    "job" : "getCell",         "config": "avgCost"},
          {"row" : 2, "col" : 4, "width" : 3, "height" : 2, "widget" : "mealDisplay",    "job" : "getCellChores",         "config": "test"},
          {"row" : 2, "col" : 1, "width" : 3, "height" : 3, "widget" : "calendar",     "job" : "google-calendar",     "config": "calendar"},
          {"row" : 4, "col" : 4, "width" : 3, "height" : 1, "widget" : "clockManManor",     "job" : "clock",     "config": "clock"}
      ]
  },

  "config" : {
    "clock":{
            "interval":1000,
            "stfuOnly":false,
            "stfuHours":[
                {
                    "start":1730,
                    "end":800
                }
            ]
        },
    "calendar" : {
       "interval" : 60000,
       "calendarUrl" : "https://www.google.com/calendar/ical/manmanoroxford%40gmail.com/private-4a70c4f939a1f673098b5b6921572cd4/basic.ics",
       "maxEntries" : 9,
       "widgetTitle" : "Events"
     },
     "owedTom" : {
        "interval" : 60000,
        "row": 0,
        "widgetTitle" : "Tom Owes"
      },
     "owedRob" : {
        "interval" : 60000,
        "row": 1,
        "widgetTitle" : "Rob Owes"
      },
     "owedYutao" : {
        "interval" : 60000,
        "row": 2,
        "widgetTitle" : "Yutao Owes"
      },
     "owedJohn" : {
        "interval" : 60000,
        "row": 3,
        "widgetTitle" : "John Owes"
      },
     "owedJacob" : {
        "interval" : 60000,
        "row": 4,
        "widgetTitle" : "Jacob Owes"
      },
     "avgCost" : {
        "interval" : 60000,
        "row": 5,
        "widgetTitle" : "Average Cost"
      },
     "test" : {
        "interval" : 360000,
        "widgetTitle" : "Meal Display",
        "request": "meal"
      }

  }
}